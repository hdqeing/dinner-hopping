    "@midwayjs/validate": "^3.0.0",
    "@sendgrid/mail": "^7.7.0",
    "@types/mjml": "^4.7.0",
    "@types/node-fetch": "^2.6.2",
    "dotenv": "^16.0.3",
    "handlebars": "^4.7.7",
    "jsonwebtoken": "^9.0.0",
    "mjml": "^4.13.0",
    "mysql2": "^3.3.0",
    "typeorm": "^0.3.15"
